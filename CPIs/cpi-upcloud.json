      "create_backup": {
        "target": {
    "Command": {
        "command": "upctl storage backup-create {storage_uuid} --title {title} --format json",
        "in_vm": false
    }
},
        "params": [
          "storage_uuid",
          "title"
        ],
        "parse_rules": {
          "type": "properties",
          "patterns": {
            "uuid": {
              "regex": "\"uuid\":\\s*\"([^\"]+)\"",
              "group": 1
            },
            "title": {
              "regex": "\"title\":\\s*\"([^\"]+)\"",
              "group": 1
            },
            "state": {
              "regex": "\"state\":\\s*\"([^\"]+)\"",
              "group": 1
            },
            "created": {
              "regex": "\"created\":\\s*\"([^\"]+)\"",
              "group": 1
            },
            "size": {
              "regex": "\"size\":\\s*([\\d]+)",
              "group": 1,
              "transform": "number"
            }
          }
        }
      },
      "list_backups": {
        "target": {
    "Command": {
        "command": "upctl storage backup-list --format json",
        "in_vm": false
    }
},
        "params": [],
        "parse_rules": {
          "type": "object",
          "patterns": {
            "backups": {
              "regex": "(\\[.*\\])",
              "group": 1
            }
          }
        }
