      "create_firewall_rule": {
        "target": {
    "Command": {
        "command": "upctl firewall rule create {server_uuid} --action {action} --direction {direction} --family {family} --position {position} --protocol {protocol} {destination_param} {source_param} {port_param} {comment_param} --format json",
        "in_vm": false
    }
},
        "params": [
          "server_uuid",
          "action",
          "direction",
          "family",
          "position",
          "protocol",
          "destination_param",
          "source_param",
          "port_param",
          "comment_param"
        ],
        "parse_rules": {
          "type": "properties",
          "patterns": {
            "position": {
              "regex": "\"position\":\\s*([\\d]+)",
              "group": 1,
              "transform": "number"
            },
            "action": {
              "regex": "\"action\":\\s*\"([^\"]+)\"",
              "group": 1
            },
            "direction": {
              "regex": "\"direction\":\\s*\"([^\"]+)\"",
              "group": 1
            },
            "protocol": {
              "regex": "\"protocol\":\\s*\"([^\"]+)\"",
