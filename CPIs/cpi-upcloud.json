{
    "name": "upcloud",
    "type": "command",
    "default_settings": {
      "zone": "fi-hel1",
      "plan": "1xCPU-1GB",
      "os": "Ubuntu Server 22.04 LTS (Jammy Jellyfish)",
      "storage_size": "25",
      "login_user": "root",
      "ssh_key": "",
      "hostname": "server1",
      "timezone": "UTC"
    },
    "actions": {
      "test_install": {
        "target": {
    "Command": {
        "command": "upctl version",
        "in_vm": false
    }
},
        "params": [],
        "parse_rules": {
          "type": "object",
          "patterns": {
            "version": {
              "regex": "upctl version ([\\d\\.]+)",
              "group": 1
            }
          }
        }
      },
      "list_workers": {
        "target": {
    "Command": {
        "command": "upctl server list --format json",
        "in_vm": false
    }
},
        "params": [],
        "parse_rules": {
          "type": "object",
          "patterns": {
            "servers": {
              "regex": "(\\[.*\\])",
              "group": 1
            }
          }
        }
      },
      "get_worker": {
        "target": {
    "Command": {
        "command": "upctl server show {server_uuid} --format json",
        "in_vm": false
    }
},
        "params": [
          "server_uuid"
        ],
        "parse_rules": {
          "type": "properties",
          "patterns": {
            "uuid": {
              "regex": "\"uuid\":\\s*\"([^\"]+)\"",
              "group": 1
            },
            "hostname": {
              "regex": "\"hostname\":\\s*\"([^\"]+)\"",
              "group": 1
            },
            "title": {
              "regex": "\"title\":\\s*\"([^\"]+)\"",
              "group": 1
            },
            "zone": {
              "regex": "\"zone\":\\s*\"([^\"]+)\"",
              "group": 1
            },
            "state": {
              "regex": "\"state\":\\s*\"([^\"]+)\"",
              "group": 1
            },
            "memory": {
              "regex": "\"memory\":\\s*\"([^\"]+)\"",
              "group": 1
            },
            "cores": {
              "regex": "\"cores\":\\s*([\\d]+)",
              "group": 1,
              "transform": "number"
            },
            "ip_addresses": {
              "regex": "\"ip_addresses\":\\s*(\\[.*?\\])",
              "group": 1
            }
          }
        }
      },
      "create_worker": {
        "target": {
    "Command": {
        "command": "upctl server create --hostname {hostname} --title {title} --zone {zone} --plan {plan} --storage-size {storage_size} --os {os} --login-user {login_user} {ssh_key_param} --format json",
        "in_vm": false
    }
},
        "params": [
          "hostname",
          "title",
          "zone",
          "plan",
          "storage_size",
          "os",
          "login_user",
          "ssh_key_param"
        ],
        "parse_rules": {
          "type": "properties",
          "patterns": {
            "uuid": {
              "regex": "\"uuid\":\\s*\"([^\"]+)\"",
              "group": 1
            },
            "hostname": {
              "regex": "\"hostname\":\\s*\"([^\"]+)\"",
              "group": 1
            },
            "title": {
              "regex": "\"title\":\\s*\"([^\"]+)\"",
              "group": 1
            },
            "zone": {
              "regex": "\"zone\":\\s*\"([^\"]+)\"",
              "group": 1
            },
            "state": {
              "regex": "\"state\":\\s*\"([^\"]+)\"",
              "group": 1
            },
            "memory": {
              "regex": "\"memory\":\\s*\"([^\"]+)\"",
              "group": 1
            },
            "cores": {
              "regex": "\"cores\":\\s*([\\d]+)",
              "group": 1,
              "transform": "number"
            },
            "ip_addresses": {
              "regex": "\"ip_addresses\":\\s*(\\[.*?\\])",
              "group": 1
            }
          }
        }
      },
      "delete_worker": {
        "target": {
    "Command": {
        "command": "upctl server delete {server_uuid}",
        "in_vm": false
    }
},
        "params": [
          "server_uuid"
        ],
        "parse_rules": {
          "type": "object",
          "patterns": {
            "success": {
              "regex": ".*",
              "transform": "boolean"
            }
          }
        }
      },
      "start_worker": {
        "target": {
    "Command": {
        "command": "upctl server start {server_uuid}",
        "in_vm": false
    }
},
        "params": [
          "server_uuid"
        ],
        "parse_rules": {
          "type": "object",
          "patterns": {
            "success": {
              "regex": ".*",
              "transform": "boolean"
            }
          }
        }
      },
      "stop_worker": {
        "target": {
    "Command": {
        "command": "upctl server stop {server_uuid} {stop_type_param}",
        "in_vm": false
    }
},
        "params": [
          "server_uuid",
          "stop_type_param"
        ],
        "parse_rules": {
          "type": "object",
          "patterns": {
            "success": {
              "regex": ".*",
              "transform": "boolean"
            }
          }
        }
      },
      "restart_worker": {
        "target": {
    "Command": {
        "command": "upctl server restart {server_uuid} {stop_type_param}",
        "in_vm": false
    }
},
        "params": [
          "server_uuid",
          "stop_type_param"
        ],
        "parse_rules": {
          "type": "object",
          "patterns": {
            "success": {
              "regex": ".*",
              "transform": "boolean"
            }
          }
        }
      },
      "modify_worker": {
        "target": {
    "Command": {
        "command": "upctl server modify {server_uuid} {plan_param} {hostname_param} {title_param} --format json",
        "in_vm": false
    }
},
        "params": [
          "server_uuid",
          "plan_param",
          "hostname_param",
          "title_param"
        ],
        "parse_rules": {
          "type": "properties",
          "patterns": {
            "uuid": {
              "regex": "\"uuid\":\\s*\"([^\"]+)\"",
              "group": 1
            },
            "hostname": {
              "regex": "\"hostname\":\\s*\"([^\"]+)\"",
              "group": 1
            },
            "title": {
              "regex": "\"title\":\\s*\"([^\"]+)\"",
              "group": 1
            },
            "plan": {
              "regex": "\"plan\":\\s*\"([^\"]+)\"",
              "group": 1
            }
          }
        }
      },
      "list_volumes": {
        "target": {
    "Command": {
        "command": "upctl storage list --format json",
        "in_vm": false
    }
},
        "params": [],
        "parse_rules": {
          "type": "object",
          "patterns": {
            "storages": {
              "regex": "(\\[.*\\])",
              "group": 1
            }
          }
        }
      },
      "get_volume": {
        "target": {
    "Command": {
        "command": "upctl storage show {storage_uuid} --format json",
        "in_vm": false
    }
},
        "params": [
          "storage_uuid"
        ],
        "parse_rules": {
          "type": "properties",
          "patterns": {
            "uuid": {
              "regex": "\"uuid\":\\s*\"([^\"]+)\"",
              "group": 1
            },
            "title": {
              "regex": "\"title\":\\s*\"([^\"]+)\"",
              "group": 1
            },
            "size": {
              "regex": "\"size\":\\s*([\\d]+)",
              "group": 1,
              "transform": "number"
            },
            "type": {
              "regex": "\"type\":\\s*\"([^\"]+)\"",
              "group": 1
            },
            "zone": {
              "regex": "\"zone\":\\s*\"([^\"]+)\"",
              "group": 1
            },
            "state": {
              "regex": "\"state\":\\s*\"([^\"]+)\"",
              "group": 1
            }
          }
        }
      },
      "create_volume": {
        "target": {
    "Command": {
        "command": "upctl storage create --title {title} --zone {zone} --size {size} --tier {tier} --format json",
        "in_vm": false
    }
},
        "params": [
          "title",
          "zone",
          "size",
          "tier"
        ],
        "parse_rules": {
          "type": "properties",
          "patterns": {
            "uuid": {
              "regex": "\"uuid\":\\s*\"([^\"]+)\"",
              "group": 1
            },
            "title": {
              "regex": "\"title\":\\s*\"([^\"]+)\"",
              "group": 1
            },
            "size": {
              "regex": "\"size\":\\s*([\\d]+)",
              "group": 1,
              "transform": "number"
            },
            "tier": {
              "regex": "\"tier\":\\s*\"([^\"]+)\"",
              "group": 1
            },
            "zone": {
              "regex": "\"zone\":\\s*\"([^\"]+)\"",
              "group": 1
            },
            "state": {
              "regex": "\"state\":\\s*\"([^\"]+)\"",
              "group": 1
            }
          }
        }
      },
      "delete_volume": {
        "target": {
    "Command": {
        "command": "upctl storage delete {storage_uuid}",
        "in_vm": false
    }
},
        "params": [
          "storage_uuid"
        ],
        "parse_rules": {
          "type": "object",
          "patterns": {
            "success": {
              "regex": ".*",
              "transform": "boolean"
            }
          }
        }
      },
      "attach_volume": {
        "target": {
    "Command": {
        "command": "upctl server storage-attach {server_uuid} --storage {storage_uuid} --type {attachment_type} --address {attachment_address} --format json",
        "in_vm": false
    }
},
        "params": [
          "server_uuid",
          "storage_uuid",
          "attachment_type",
          "attachment_address"
        ],
        "parse_rules": {
          "type": "object",
          "patterns": {
            "success": {
              "regex": ".*",
              "transform": "boolean"
            }
          }
        }
      },
      "detach_volume": {
        "target": {
    "Command": {
        "command": "upctl server storage-detach {server_uuid} --address {attachment_address}",
