},
        "params": [],
        "parse_rules": {
          "type": "object",
          "patterns": {
            "plans": {
              "regex": "(\\[.*\\])",
              "group": 1
            }
          }
        }
      },
      "list_regions": {
        "target": {
    "Command": {
        "command": "upctl zone list --format json",
        "in_vm": false
    }
},
        "params": [],
        "parse_rules": {
          "type": "object",
          "patterns": {
            "zones": {
              "regex": "(\\[.*\\])",
              "group": 1
            }
          }
        }
      },
      "list_templates": {
        "target": {
    "Command": {
        "command": "upctl template list --format json",
        "in_vm": false
    }
},
        "params": [],
        "parse_rules": {
          "type": "object",
          "patterns": {
            "templates": {
              "regex": "(\\[.*\\])",
              "group": 1
            }
          }
        }
