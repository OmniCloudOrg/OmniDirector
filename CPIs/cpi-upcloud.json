        "parse_rules": {
          "type": "object",
          "patterns": {
            "rules": {
              "regex": "(\\[.*\\])",
              "group": 1
            }
          }
        }
      },
      "delete_firewall_rule": {
        "target": {
    "Command": {
        "command": "upctl firewall rule delete {server_uuid} {position}",
